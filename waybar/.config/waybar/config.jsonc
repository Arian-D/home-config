{
    "layer": "top",
    "modules-left": ["river/mode", "river/tags"],
    "modules-center": ["river/window"],
    "modules-right": ["backlight/slider", "wireplumber", "battery", "clock"],
    "backlight/slider": {
        "min": 10,
        "max": 100
    },
    "wireplumber": {
        "format": "{volume}% {icon}",
        "format-muted": "",
        "on-click": "helvum",
        "format-icons": ["", "", ""]
    },
    "battery": {
        "format": "{capacity}% {icon}",
        "format-icons": ["", "", "", "", ""]
    },
    "clock": {
        "format-alt": "{:%a, %d. %b  %H:%M}"
    },
    "river/mode": {
        "format": "{}"
    },
    "river/tags": {
        "num-tags": 9
    }
}